{
  "name": "oc-vault-landing",
  "version": "1.0.0",
  "description": "OC Vault - Professional Photographer's Cloud Backup Landing Page",
  "scripts": {
    "dev": "npx serve . -l 3000",
    "start": "npx serve . -l 3000",
    "deploy": "wrangler pages deploy . --project-name=oc-vault",
    "setup-db": "npx wrangler d1 execute oc-vault-db --remote --file=./schema.sql",
    "db:local": "npx wrangler d1 execute oc-vault-db --local --file=./schema.sql",
    "db:query": "npx wrangler d1 execute oc-vault-db --remote --command",
    "wrangler": "wrangler"
  },
  "keywords": [
    "photography",
    "cloud-storage",
    "backup",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.2.1"
  },
  "dependencies": {
    "@vercel/kv": "^3.0.0"
  }
}
